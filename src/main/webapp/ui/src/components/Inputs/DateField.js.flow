import { type Node } from "react";

export type DateFieldArgs = {|
  value: ?string,
  label?: Node,
  alert?: Node,
  disabled?: boolean,
  noValueLabel?: ?string,
  onChange?: ({ target: { value: ?Date } }) => void,
  minDate?: Date,
  maxDate?: Date,
  disableFuture?: boolean,
  placeholder?: string,
  datatestid?: string,
  variant?: "standard" | "outlined",
  id?: string,
  disableWidthLimit?: boolean,
