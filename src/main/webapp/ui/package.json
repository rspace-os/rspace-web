  "jest": {
    "moduleNameMapper": {
      "^Styles$": "<rootDir>/src/util/styles.ts",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.(css)$": "<rootDir>/src/test-stubs/CSSStub.js",
      "^.+\\.(jpg|png)$": "<rootDir>/src/test-stubs/ImageStub.js",
      "^.+\\.(svg)$": "<rootDir>/src/test-stubs/SVGStub.js",
      "react-photoswipe-gallery": "<rootDir>/src/test-stubs/PhotoswipeStub.js",
      "^__mocks__/(.*)$": "<rootDir>/__mocks__/$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs)$": [
        "babel-jest",
        {
          "extends": "./babel.config.json"
        }
      ],
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?*.test.(js|cjs|mjs|jsx|ts|tsx)"
    ],
    "testTimeout": 20000,
    "reporters": [
      "default",
      "jest-junit",
      "<rootDir>/__tests__/reporters/slowest"
    ],
    "testEnvironmentOptions": {
      "resources": "usable"
    }
  }
