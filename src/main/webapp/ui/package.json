  "jest": {
    "moduleNameMapper": {
      "^Styles$": "<rootDir>/src/util/styles.ts",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.(css)$": "<rootDir>/src/test-stubs/CSSStub.js",
      "^.+\\.(jpg)$": "<rootDir>/src/test-stubs/JPGStub.js",
      "^.+\\.(svg)$": "<rootDir>/src/test-stubs/SVGStub.js",
      "react-photoswipe-gallery": "<rootDir>/src/test-stubs/PhotoswipeStub.js",
      "^__mocks__/(.*)$": "<rootDir>/__mocks__/$1"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "diagnostics": {
            "ignoreCodes": [
              1343
            ]
          },
          "astTransformers": {
            "before": [
              {
                "path": "node_modules/ts-jest-mock-import-meta",
                "options": {
                  "metaObjectReplacement": {
                    "url": "https://www.url.com"
                  }
                }
              }
            ]
          }
        },
        "tsconfig.json"
      ]
    },
    "testMatch": [
      "**/?*.test.(js|ts|tsx)"
    ],
    "testTimeout": 20000,
    "reporters": [
      "default",
      "jest-junit",
      "<rootDir>/__tests__/reporters/slowest"
    ],
    "testEnvironmentOptions": {
      "resources": "usable"
    }
  }
}
